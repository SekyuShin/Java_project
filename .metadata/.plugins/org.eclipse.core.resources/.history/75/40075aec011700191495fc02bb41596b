package ex;

public class Num_Char {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		System.out.println('생');
		//한글자는 작은 따음표
		System.out.println("생활코딩");
		//문자열은 큰 따음표
		//System.out.println('생활코딩');
		//에러 예제
		System.out.println("I tell you \"hello\"");
		// \"를 씀으로써 문자로 해석하게 만듬
		System.out.println("생활"+"코딩");
		// +연산자를 씀으로써, 문자열 연결 가능
	}

}
