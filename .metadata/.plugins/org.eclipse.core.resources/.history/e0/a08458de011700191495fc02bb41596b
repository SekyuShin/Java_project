package ex;

public class Num_Char {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		System.out.println('생');
		//한글자는 작은 따음표
		System.out.println("생활코딩");
		//문자열은 큰 따음표
		//System.out.println('생활코딩');
		//에러 예제
		System.out.println("I tell you \"hello\"");
	
	}

}
